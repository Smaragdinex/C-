#include <stdio.h>
#include <stdlib.h>

int min(int arr[]);

int main(void)
{
    int arr[5] = {3,0,9,8,2};
    
    printf("%d",min(arr));
    return 0;
}

int min(int arr[])
{
    int i;
    int min = arr[0];
    
    for(i=0; i < 5; i++)
    {
        if(min > arr[i])
        {
            min = arr[i];
        }
    }
    return min;
}

