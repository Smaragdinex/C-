#include <stdio.h>
#include <stdlib.h>
#define ROW 4
#define COL 3

int main(void)
{
    int i,j;
    int A[ROW][COL] = {{1,2,3},{5,6,7},{3,4,6},{5,6,7}};
    int B[ROW][COL] = {{6,7,8},{3,6,7},{3,8,5},{8,9,2}};
    printf("MArtix A+B =\n");
    for(i=0;i<ROW;i++)
    {
        for(j=0;j<COL;j++)
        {
            printf("%3d",A[i][j] + B[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}

