#include <stdio.h>
#include <stdlib.h>
// 用for迴圈 計算 1² - 2² + 3² - 4² + 5² - 6² + ... + 49² - 50² 總和

int main(void)
{
    int i, sum=0;
    
    for(i = 1; i<=50; i++)
    {
        if(i%2==1)
        {
            sum+= i*i;
        }
        else
        {
            sum-= i*i;
        }
    }
    
    printf("%d",sum);
    
    return 0;
}
