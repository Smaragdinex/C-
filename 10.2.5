#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    int a = 5, b= 10;
    int *ptr1, *ptr2;
    ptr1 = &a;
    printf("%p\n", ptr1);
    printf("a address: %p\n", &a);
    printf("b address: %p\n", &b);
    ptr2 = &b;
    printf("%p\n", ptr2);
    *ptr1 = 7;
    printf("%d\n",*ptr1);
    *ptr2 = 32;
    printf("%d\n",*ptr2);
    a = 17;
    ptr1 = ptr2;
    printf("%p\n",ptr1);
    *ptr1 = 9;
    printf("%d\n",*ptr1);
    ptr1 = &a;
    printf("%p\n",ptr1);
    a = 64;
    *ptr2 = *ptr1 +5;
    printf("%d\n",*ptr2);
    ptr2 = &a;
    printf("%p\n",ptr2);
    printf("%p\n",ptr1);
  
    return 0;
}

