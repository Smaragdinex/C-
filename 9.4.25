#include <stdio.h>
#include <stdlib.h>

int main(void)
{
   char str[] = "Hello, C language";
   
   int i;
       
    for(i=0; str[i] != '\0'; i++)
    {
        if(str[i] >= 'A' && str[i] <= 'Z')
        {
            str[i] = str[i] + 32;
        }
    }
    
    printf("%s",str);
    
    return 0;
}

 
