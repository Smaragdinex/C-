#include <stdio.h>
//向右靠齊」的數字三角形
//    1
//   12
//  123
// 1234
//12345

int main(void)
{
    int i,j,s;
    
    for(i=1; i<=5; i++)
    {
        for(s=1;s<=5-i;s++)
        {
            printf(" ");
        }
        for(j=1;j<=i;j++)
        {
        printf("%d",j);
        }
        printf("\n");
    }
    
    return 0;
}
