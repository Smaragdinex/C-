#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    int a = 5, b= 10;
    int *ptr1, *ptr2;
    printf("%p\n",ptr1);
    printf("%d\n",*ptr1);
    ptr1 = &a;
    printf("%p\n", ptr1);
    printf("a address: %p\n", &a);
    printf("b address: %p\n", &b);
    printf("%d\n",*ptr1);
    ptr2 = &b;
    printf("%p\n", ptr2);
    *ptr1 = 7;
    printf("%d\n",*ptr1);
    printf("a : %d\n",a);
    ptr1 = &a;
    printf("a: %d\n",a);
    printf("ptr1: %p\n",ptr1);
    printf("ptr1: %d\n",*ptr1);
    a = b;
    printf("a = b: \n");
    printf("a: %d\n",a);
    printf("ptr1: %p\n",ptr1);
    printf("ptr1: %d\n",*ptr1);
    *ptr1 = 32;
    printf("32 : \n");
    printf("%d\n",*ptr1);
    printf("a: %d\n",a);
    printf("ptr1: %p\n",ptr1);
    printf("b: %d\n",b);
    printf("*************\n");
    a = 17;
    ptr1 = ptr2;
    printf("%p\n",ptr1);
    *ptr1 = 9;
    printf("%d\n",*ptr1);
    ptr1 = &a;
    printf("%p\n",ptr1);
    a = 64;
    *ptr2 = *ptr1 +5;
    printf("%d\n",*ptr2);
    ptr2 = &a;
    printf("%p\n",ptr2);
    printf("%p\n",ptr1);
  
    return 0;
}

