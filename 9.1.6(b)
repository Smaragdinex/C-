#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    int count = 0;
    
    int array[] = {3,5,0,3,2,4,1,6,8,5,4,3,2};
    
    printf("%ld\n", sizeof(array) / sizeof(int));
    
    for(int i =0; i < 13; i++)
    {
        if(array[i] >=3 && array[i] <= 6)
        {
            count++;
        }
    }
    
    printf("total (3~6): %d\n",count);
}
