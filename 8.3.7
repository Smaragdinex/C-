#include <stdlib.h> //standrad library
#include <stdio.h> //standrad input/output

int prime(int n); //function prototype and definition

int main(void)
{
  int n = 100;
  printf("%d",prime(n));

  return 0;
}

int prime(int n)
{
  int count = 0;
  int i = 2;

  while(1)
  {
    int isPrime = 1;
    for(int j = 2; j*j <=i; j++)
    {
      if(i % j == 0)
        { 
          isPrime = 0;
          break;
        }
    }

    if(isPrime)
    {
      count++;
      if(count == n)
        return i;
    }
      i++;
  }
}
