#include <stdio.h>
#include <stdlib.h>
int count(char str[]);
int main(void)
{
   char str[] = "Hello, C language";
   
    printf("%d",count(str));
    
    return 0;
}
int count(char str[])
{
    int count = 0;
    
    while(str[count] != '\0')
    {
        count++;
    }
    return count;
}

 
